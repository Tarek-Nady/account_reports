<?xml version="1.0" encoding="UTF-8" ?>
<templates>
    <t t-name="account_reports_check_number.GeneralLedgerLineName" t-inherit="account_reports.GeneralLedgerLineName">
        <xpath expr="//div[@data-id='content']" position="inside">
            <t t-if="props.line.check_number">
                <div class="check_number_info">
                    <span class="text-muted">Check: </span>
                    <span class="check_number_value" t-out="props.line.check_number"/>
                </div>
            </t>
        </xpath>
    </t>
</templates>
